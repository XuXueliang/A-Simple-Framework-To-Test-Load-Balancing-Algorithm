	}
	
#tab-2 table.importone span input[type=text]{
	height:14px;
	margin-top:-2px;
	margin-left:5px;
	}	
#tab-2 table.importone span label{
	margin-right:10px;
	}	
#tab-2 table.importone span input[type=radio]{
	height:14px;
	margin-top:-1px;
	}		

#tab-2 table.importone span input[type=button]#button1{
	background-color:#426EB4;
	padding:3px 15px;
	color:white;
