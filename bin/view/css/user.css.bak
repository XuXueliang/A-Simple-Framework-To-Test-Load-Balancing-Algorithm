	}	
			
/*user_student tab-3*/	
#tab-3 div.studentlist{
	width:98%;
	height:450px;
	margin:20px 1% 0 1%;
	border:1px solid #ccc;
	overflow:auto;
	}	
#tab-3 table.list{
	border-collapse:collapse;
	font-size:12px;
	}
#tab-3 table.list th{
	padding:3px 6px;
	color:white;
	background:#6495ED;
	border-bottom:1px solid #ccc;
	}	
#tab-3 table.list td{
	padding:7px;
	border-bottom:1px solid #ccc;
	font-size:13px;
	text-align:center;
	}		
#tab-3 table.list tr:hover{
	background:#87CEFA;
	color:white;
	}	
#tab-3 table.list td a{
	text-decoration:none;
	color:inherit;
	}	
#tab-3 table.list td a:hover{
	text-decoration:underline;
	color:#005687;
	}	
#tab-3 .deletebatch{
	font-size:16px;
	font-family:"华文楷体";
	width:98%;
	margin:20px 1% 0 1%;
	text-indent:7px;
	color:#005687;
	}
#tab-3 .deletebatch select{
	margin-left:10px;
	}	
#tab-3 .deletebatch a{
	background-color:#426EB4;
	padding:1px 10px;
	color:white;
	border:1px solid #ccc;
	margin-left:10px;
	text-decoration:none;
	}	
#tab-3 .deletebatch a i{
	margin-top:2px;
	margin-right:3px;
	}	
#tab-3 .deletebatch a:hover{
	background-color:#06B9D1;
	}

