@charset "utf-8";
/* CSS Document */


/*info edit*/
.clickedit{
	float:left;
	width:84%;
	margin:30px 8% 0 8%;
	font-size:14px;
	height:24px;
	line-height:24px;
	}
.clickedit a{
	float:right;
	text-decoration:none;
	color:#2A8FBD;
	}
.clickedit a:hover{
	text-decoration:underline;
	}	
	
.info{
	float:left;
	width:84%;
	margin:20px 8% 0 8%;
	font-size:14px;
	}
.info .infohead{
	width:100%;
	border-bottom:1px solid #2A8FBD;
	font-family:"Adobe 黑体 Std R";
    font-weight:bold;
	text-indent:10px;
	height:24px;
	line-height:24px;
	} 	
	
.info table{
	border-collapse:collapse;
	margin-top:10px;
	}
.info table td{
	padding:5px;
	}	
.info table td span{
	margin-left:10px;
	color:#2A8FBD;
	}	

/*hidden*/
.pleaseedit{
	display:none;
	float:left;
	width:84%;
	margin:30px 8% 0 8%;
	font-size:13px;
	height:24px;
	line-height:24px;
	border:1px solid #6EC3C9;
	background-color:#FEEBD0;
	text-indent:10px;
	}	

.info2{
	display:none;
	float:left;
	width:84%;
	margin:20px 8% 0 8%;
	font-size:14px;
	border:1px solid #6EC3C9;
	}	

.info2 .infohead{
	width:100%;
	font-family:"Adobe 黑体 Std R";
    font-weight:bold;
	height:24px;
	line-height:24px;
	text-indent:10px;
	background:#BFCAE6;
	} 	
.info2 table{
	border-collapse:collapse;
	margin-top:10px;
	margin-bottom:10px;
	}
.info2 table td{
	padding:5px;
	color:grey;
	font-weight:bold;
	font-size:13px;
	}	
.info2 table input[type=button]#button1{
	background-color:#00A6AD;
	color:white;
	margin-right:10px;
	}	
.info2 table input[type=button]#button2{
	background-color:white;
	}		
	
/*psw_change*/						
div#psw_change{
	display:block;
	width:84%;
	margin:30px 8% 0 8%;
	font-size:14px;
	}
#psw_change_head{
	width:100%;
	border-bottom:1px solid #2A8FBD;
	font-family:"Adobe 黑体 Std R";
    font-weight:bold;
	height:24px;
	line-height:24px;
	text-indent:10px;
	}		
	
#psw_change_content{
	width:100%;
	margin:5px 0;
	}	
#psw_change_content table{
	margin-top:10px;
	}		
#psw_change_content table td{
	padding:8px 0;
	font-size:14px;
	color:#666;
	}		
#psw_change_content table td input[type=password]{
	height:25px;
	width:200px;
	font-size:14px;
	line-height:25px;
	border:1px solid #CCC;
	float:left; 
	}		
#psw_change_content table td input:focus{
	background:#E6F1D8;
	outline:none;
	border:1px solid #98D0B9; 
	}	
#psw_change_content table td span{
	display:none;
	float:left;
	background:#E6F1D8;
	margin-left:20px;
	min-height:27px;
	max-width:200px;
	line-height:27px;
	padding:0 10px;
	border-radius:5px;
	color:black;
	font-size:12px;
	}	
#psw_change_content table td input[type=button]{
	border:none;
	color:#fff;
	background:#06B9D1;
	font-family:"Adobe 宋体 Std L";
	border-radius:5px;
	padding:5px 15px;
	}	
#psw_change_content table td input[type=button]:hover{
	background:#00AE72;
	cursor:pointer;
	}

/*for Teacher
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~*/	
#user_tabs{
	width:680px;
	margin:10px 9px;
	float:left;
	border:1px solid #ccc;
	}

	
#user_tabs ul{
	list-style:none;
	margin:0;
	padding:0;
	width:100%;
	height:35px;
	background:#94AAD6;
	}
#user_tabs ul li{
	display:block;
	float:left;
	margin:6px 0 0 3px;
	height:27px;
	line-height:27px;
	padding:0 10px;
	background:#BFCAE6;
	border: 1px solid #94AAD6;
	font-family:"幼圆";
	}
	
#user_tabs li:hover{
	background:#426EB4;
	border: 1px solid #6EC3C9;
	color:white;
	}		
#user_tabs li a{
	text-decoration:none;
	color:inherit;
	display:block;
	float:left;
	height:25px;
	}	
#user_tabs li.current{
	background:#FDFCF9;
	border:1px solid white;
	color:#205AA7;
	}	
	
#user_tabs div.tabdiv{
	background:#FDFCF9;
	display:none;
	width:100%;
	float:left;
	height:540px;
	}		
#user_tabs div.current{
	display:block;
	}
	
/*user_student tab-1*/	
#tab-1 table.importbatch{
	width:90%;
	margin:20px 5% 0 5%;
	font-family:"Adobe 宋体 Std L";
	font-size:14px;
	border-collapse:collapse;
	}
	
#tab-1 table.importbatch td{
	padding:20px 5px;
	vertical-align:baseline;
	}
	
#tab-1 table.importbatch span{
	float:right;
	width:450px;
	margin-right:20px;
	color:#1A4FA3;
	}
#tab-1 table.importbatch span select{
	margin-top:-2px;
	}	
#tab-1 table.importbatch span input[type=text]{
	height:14px;
	margin-top:-2px;
	margin-left:5px;
	}	
#tab-1 table.importbatch span label{
	margin-right:10px;
	}	
#tab-1 table.importbatch span input[type=radio]{
	height:14px;
	margin-top:-1px;
	}		
#tab-1 table.importbatch span input[type=file]{
	display:block;
	float:left;
	margin-top:-5px;
	margin-bottom:5px;
	border:1px #ccc solid;
	width:255px;
	}	
#tab-1 table.importbatch span input[type=button].upload{
	margin-left:10px;
	margin-top:-4px;
	}
#tab-1 table.importbatch span input[type=button]#button1{
	background-color:#426EB4;
	padding:3px 15px;
	color:white;
	border:1px solid #ccc;
	cursor:pointer;
	margin-right:10px;
	}	
#tab-1 table.importbatch span input[type=button]#button2{
	background-color:white;
	margin-right:10px;
	padding:3px 15px;
	border:1px solid #ccc;
	cursor:pointer;
	}		
/*user_student tab-2*/	
#tab-2 table.importone{
	width:90%;
	margin:20px 5% 0 5%;
	font-family:"Adobe 宋体 Std L";
	font-size:14px;
	border-collapse:collapse;
	}
	
#tab-2 table.importone td{
